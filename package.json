{
  "name": "fujimi-ui",
  "version": "0.0.8",
  "repository": "ssh://git@github.com/tricot-inc/fujimi-ui.git",
  "author": "konojunya <junya03dance@gmail.com>",
  "license": "MIT",
  "main": "lib/index.js",
  "module": "esm/index.js",
  "typings": "lib/index.d.ts",
  "sideEffects": false,
  "engines": {
    "node": ">=v10.0.0"
  },
  "files": [
    "esm",
    "lib"
  ],
  "scripts": {
    "build": "run-p build:*",
    "build:lib": "tsc -p tsconfig.build.json",
    "build:esm": "tsc -p tsconfig.esm.build.json",
    "clean": "rimraf ./lib ./esm",
    "format": "eslint --fix './**/*.ts{,x}'",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint './**/*.ts{,x}'",
    "lint:tsc": "tsc --noEmit",
    "build-storybook": "build-storybook --quiet",
    "storybook": "start-storybook -p 6006 -s ./public",
    "test": "jest",
    "prepublishOnly": "run-s clean lint build",
    "release": "standard-version",
    "publish": "git push --follow-tags origin head && npm publish",
    "release:dryrun": "standard-version --dry-run"
  },
  "devDependencies": {
    "@babel/core": "7.16.12",
    "@commitlint/cli": "16.1.0",
    "@commitlint/config-conventional": "16.0.0",
    "@next/eslint-plugin-next": "12.0.8",
    "@storybook/addon-a11y": "6.4.14",
    "@storybook/addon-essentials": "6.4.14",
    "@storybook/addons": "6.4.14",
    "@storybook/cli": "6.4.14",
    "@storybook/react": "6.4.14",
    "@swc/core": "1.2.135",
    "@swc/jest": "0.2.17",
    "@types/jest": "^27.4.0",
    "@types/node": "17.0.10",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/styled-components": "5.1.21",
    "@typescript-eslint/eslint-plugin": "5.10.0",
    "@typescript-eslint/parser": "5.10.0",
    "eslint": "8.8.0",
    "eslint-config-next": "12.0.8",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-jest": "26.1.1",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.28.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-storybook": "0.5.6",
    "gts": "3.1.0",
    "jest": "27.4.7",
    "next": "11.1.3",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "rimraf": "3.0.2",
    "standard-version": "9.3.2",
    "styled-components": "5.3.3",
    "typescript": "4.5.5"
  },
  "dependencies": {
    "use-intersection": "^0.2.1"
  },
  "peerDependencies": {
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "5.3.3",
    "next": "11.1.3"
  }
}
