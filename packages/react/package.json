    "@storybook/react": "6.4.14",
    "@swc/core": "1.2.135",
    "@swc/jest": "0.2.20",
    "@types/react": "17.0.44",
    "@types/react-dom": "17.0.15",
    "@types/styled-components": "5.1.21"
  },
  "dependencies": {
    "react": "17.0.2",
